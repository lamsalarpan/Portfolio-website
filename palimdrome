#include <stdio.h>
#include <conio.h>
#include <string.h>
int main()
{
    int a[100],b[100];
    printf("Enter a string: ");
    scanf("%s", a);
    b=strcpy(a);
    strcpy(b);
    if (strcmp(a,b)==0)
        printf("The string is a palindrome.\n");
    else
        printf("The string is not a palindrome.\n");
    getch();
    return 0;
}